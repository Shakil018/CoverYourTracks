      'yes'       : '<span class="complimentary_text yes">Your browser has a non-unique fingerprint</span>',
      'almost_yes': '<span style="color: #5cb900; font-size: 15pt;">&#9685;</span><span class="complimentary_text">&nbsp;your browser has a randomized fingerprint</span>',
